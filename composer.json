{
    "name": "alphalemon/elfinder-bundle",
    "description": "The ElFinderBundle encapsulates the elFinder file manager to be used in Symfony2.",
    "keywords": ["elfinder", "ajax", "file manager", "cms", "alphalemon"],
    "homepage": "http://alphalemon.com",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
    {
      "name": "AlphaLemon",
      "email": "info@alphalemon.com"
    }
    ],
    "repositories": {
        "ElFinder": {
            "package": {
                "name": "ElFinder",
                "version": "2.0.0-beta",
                "config": {
                    "vendor-dir": "Resources/public/vendor"
                },
                "source": {
                    "url": "https://github.com/Studio-42/elFinder.git",
                    "type": "git",
                    "reference": "2.0-beta.zip"
                }
            }
        }
    },
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony":  ">=2.0.9,<2.1.0-dev",
        "ElFinder": "2.0.0-beta"
    },
    "autoload": {
        "psr-0": { "AlphaLemon\\ElFinderBundle": "" }
    }
}
